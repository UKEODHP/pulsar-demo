  "version": "0.2.0",
  "configurations": [
    {
      "name": "Pulsar Demo: Publish",
      "type": "debugpy",
      "request": "launch",
      "module": "pulsar_demo",
      "args": [
        "pulsar://127.0.0.1:6650",
        "pulsar-demo",
        "publish",
        "--prefix",
        "pub1"
      ],
      "justMyCode": false
    },
    {
      "name": "Pulsar Demo: Subscribe",
      "type": "debugpy",
      "request": "launch",
      "module": "pulsar_demo",
      "args": [
        "pulsar://127.0.0.1:6650",
        "pulsar-demo",
        "subscribe",
        "--prefix",
        "sub1"
      ],
      "justMyCode": false
    }
  ]
}
